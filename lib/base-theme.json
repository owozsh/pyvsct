    "editorLineNumber.foreground": "$FG40",
    "panelTitle.activeForeground": "$FG",
    "panelTitle.activeBorder": "$BG_BRIGHT",
    "panelTitle.inactiveForeground": "$FG70",
    "panel.border": "$FG00",
    "panel.background": "$BG_DARK",
    "settings.focusedRowBackground": "$BG_BRIGHT5A",
    "titleBar.border": "$BORDER",
    "titleBar.activeBackground": "$BG_DARK",
    "statusBar.background": "$BG_DARK",
    "statusBar.noFolderBackground": "$BG_BRIGHT",
    "statusBar.debuggingBackground": "$BG_BRIGHT",
    "statusBarItem.remoteBackground": "$BLUE70",
    "ports.iconRunningProcessForeground": "$FG",
    "activityBar.background": "$BG_DARK",
    "activityBar.foreground": "$FG",
    "sideBar.background": "$BG_DARK",
    "sideBar.foreground": "$FG80",
    "sideBarSectionHeader.background": "$BG_BASE",
    "menu.background": "$BG_BASE",
    "menu.foreground": "$FG",
    "pickerGroup.foreground": "$BG_BRIGHT",
    "input.background": "$BG_BRIGHT",
    "inputOption.activeBorder": "$BG_BRIGHT",
    "focusBorder": "$ACCENT",
    "editorWidget.background": "$BG_BRIGHT",
    "debugToolBar.background": "$BG_BASE",
    "diffEditor.insertedTextBackground": "$GREEN80",
    "diffEditor.removedTextBackground": "$RED70",
    "inputValidation.errorBackground": "$RED4A",
    "inputValidation.errorBorder": "$RED",
    "inputValidation.warningBackground": "$YELLOW",
    "inputValidation.warningBorder": "$YELLOW",
    "inputValidation.infoBackground": "$BLUE",
    "inputValidation.infoBorder": "$BLUE",
    "editorHoverWidget.background": "$BG_BRIGHT",
    "editorHoverWidget.border": "$BG_BRIGHT",
    "editorSuggestWidget.background": "$BG_BASE",
    "editorSuggestWidget.border": "$BG_BRIGHT",
    "editorGroup.border": "$BG_BRIGHT",
    "peekView.border": "$BG_BRIGHT",
    "peekViewEditor.background": "$BG_BASE",
    "peekViewResult.background": "$BG_BASE",
    "peekViewTitle.background": "$BG_BASE",
    "peekViewResult.selectionBackground": "$ACCENT30",
    "peekViewResult.matchHighlightBackground": "$BG_BRIGHT",
    "peekViewEditor.matchHighlightBackground": "$BG_BRIGHT",
    "terminal.ansiBlack": "$BLACK",
    "terminal.ansiRed": "$RED",
    "terminal.ansiGreen": "$GREEN",
    "terminal.ansiYellow": "$YELLOW",
    "terminal.ansiBlue": "$BLUE",
    "terminal.ansiMagenta": "$MAGENTA",
    "terminal.ansiCyan": "$CYAN",
    "terminal.ansiWhite": "$WHITE",
    "terminal.ansiBrightBlack": "$BLACK",
    "terminal.ansiBrightRed": "$RED",
    "terminal.ansiBrightGreen": "$GREEN",
    "terminal.ansiBrightYellow": "$YELLOW",
    "terminal.ansiBrightBlue": "$BLUE",
    "terminal.ansiBrightMagenta": "$MAGENTA",
    "terminal.ansiBrightCyan": "$CYAN",
    "terminal.ansiBrightWhite": "$WHITE"
  },
  "tokenColors": [
    {
      "settings": {
        "foreground": "$FG"
      }
    },
    {
      "scope": [
        "meta.embedded",
        "source.groovy.embedded",
        "string meta.image.inline.markdown"
      ],
      "settings": {
        "foreground": "$FG"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "$COMMENT"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "$STRING"
      }
    },
    {
      "name": "Template Definition",
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "$KEYWORD"
      }
    },
    {
      "name": "Reset JavaScript string interpolation expression",
      "scope": ["meta.template.expression"],
      "settings": {
        "foreground": "$FG"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "$PROPERTY"
      }
    },
    {
      "name": "Property",
      "scope": "variable.other.property",
      "settings": {
        "foreground": "$PROPERTY"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "$PROPERTY"
      }
    },
    {
      "name": "User-defined constant",
      "scope": "constant.character, constant.other",
      "settings": {
        "foreground": "$PROPERTY"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "fontStyle": "",
        "foreground": "$FG"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "$KEYWORD"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "fontStyle": "",
        "foreground": "$KEYWORD"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "fontStyle": "",
        "foreground": "$TYPE"
      }
    },
    {
      "name": "Class name",
      "scope": "entity.name.type, entity.name.class, entity.name.namespace, entity.name.scope-resolution",
      "settings": {
        "fontStyle": "underline",
        "foreground": "$FUNCTION"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "underline",
        "foreground": "$FUNCTION"
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {
        "fontStyle": "",
        "foreground": "$FUNCTION"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "",
        "foreground": "$PARAMETER"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": "",
        "foreground": "$KEYWORD"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": "",
        "foreground": "$FUNCTION"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "fontStyle": "",
        "foreground": "$TYPE"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "fontStyle": "",
        "foreground": "$TYPE"
      }
    },
    {
      "name": "Library class/type",
      "scope": "support.type, support.class",
      "settings": {
        "fontStyle": "",
        "foreground": "$TYPE"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "fontStyle": "",
        "foreground": "$RED"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "$RED"
      }
    },
    {
      "name": "JSON String",
      "scope": "meta.structure.dictionary.json string.quoted.double.json",
      "settings": {
        "foreground": "$FG"
      }
    },
    {
      "name": "diff.header",
      "scope": "meta.diff, meta.diff.header",
      "settings": {
        "foreground": "$BG_BRIGHT"
      }
    },
    {
      "name": "diff.deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "$KEYWORD"
      }
    },
    {
      "name": "diff.inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "$FUNCTION"
      }
    },
    {
      "name": "diff.changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "$STRING"
      }
    },
    {
      "scope": "constant.numeric.line-number.find-in-files - match",
      "settings": {
        "foreground": "$PROPERTYA0"
      }
    },
    {
      "scope": "entity.name.filename.find-in-files",
      "settings": {
        "foreground": "$STRING"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "$KEYWORD"
      }
    },
    {
      "name": "Markup Lists",
      "scope": "markup.list",
      "settings": {
        "foreground": "$STRING"
      }
    },
    {
      "name": "Markup Styling",
      "scope": "markup.bold, markup.italic",
      "settings": {
        "foreground": "$TYPE"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "fontStyle": "",
        "foreground": "$PARAMETER"
      }
    },
    {
      "name": "Markup Headings",
      "scope": "markup.heading",
      "settings": {
        "foreground": "$FUNCTION"
      }
    },
    {
      "name": "Markup Setext Header",
      "scope": "markup.heading.setext",
      "settings": {
        "foreground": "$FUNCTION",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Headings",
      "scope": "markup.heading.markdown",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": "markup.quote.markdown",
      "settings": {
        "fontStyle": "italic",
        "foreground": "$BG_BRIGHT"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": "markup.bold.markdown",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Link Title/Description",
      "scope": "string.other.link.title.markdown,string.other.link.description.markdown",
      "settings": {
        "foreground": "$PROPERTY"
      }
    },
    {
      "name": "Markdown Underline Link/Image",
      "scope": "markup.underline.link.markdown,markup.underline.link.image.markdown",
      "settings": {
        "foreground": "$STRING"
      }
    },
    {
      "name": "Markdown Emphasis",
      "scope": "markup.italic.markdown",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.strikethrough",
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Markdown Punctuation Definition Link",
      "scope": "markup.list.unnumbered.markdown, markup.list.numbered.markdown",
      "settings": {
        "foreground": "$FG"
      }
    },
    {
      "name": "Markdown List Punctuation",
      "scope": ["punctuation.definition.list.begin.markdown"],
      "settings": {
        "foreground": "$FUNCTION"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "$BLUE"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "$YELLOW"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "$RED"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "$BLUE"
      }
    },
    {
      "name": "this.self",
      "scope": "variable.language",
      "settings": {
        "foreground": "$PARAMETER"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation.separator",
      "settings": {
        "foreground": "$COMMENT"
      }
    }
  ],
  "semanticHighlighting": true
}

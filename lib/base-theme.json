    "dropdown.listBackground": "$BG_BASE",
    "list.inactiveSelectionBackground": "$BG_BRIGHT",
    "list.hoverBackground": "$BG_BRIGHT",
    "list.dropBackground": "$BG_BRIGHT",
    "list.highlightForeground": "$FG",
    "button.background": "$BG_BRIGHT",
    "editor.background": "$BG_BASE",
    "editor.foreground": "$FG",
    "selection.background": "$SELECTION",
    "editor.selectionHighlightBackground": "$SELECTION",
    "editor.selectionBackground": "$SELECTION",
    "minimap.selectionHighlight": "$SELECTION",
    "editor.wordHighlightBackground": "$BLUE80",
    "editor.wordHighlightStrongBackground": "$BLUE80",
    "editor.lineHighlightBackground": "$BG_BRIGHT",
    "editorLineNumber.activeForeground": "$ACCENT",
    "editorCursor.foreground": "$ACCENT",
    "editorWhitespace.foreground": "$FG60",
    "editorIndentGuide.background": "$FG10",
    "editorIndentGuide.activeBackground": "$FG30",
    "editorGroupHeader.tabsBackground": "$BG_DARK",
    "editorGroup.dropBackground": "$BG_BRIGHT80",
    "tab.inactiveBackground": "$BG_DARK",
    "tab.activeBackground": "$BG_BASE",
    "tab.activeBorderTop": "$ACCENT30",
    "statusBar.border": "$BORDER",
    "sideBar.border": "$BORDER",
    "tab.border": "$BORDER",
    "tab.activeBorder": "$BG_BASE",
    "editorGroupHeader.tabsBorder": "$BORDER",
    "tab.inactiveForeground": "$FG",
    "tab.lastPinnedBorder": "$BG_BRIGHT",
    "widget.shadow": "$BG_BASE00",
    "progressBar.background": "$BG_BRIGHT",
    "badge.background": "$BG_BRIGHT",
